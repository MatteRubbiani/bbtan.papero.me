dth: 100%;\r\n  z-index: -2;\r\n  .up-arrow-container {\r\n    @media (min-width: 700px) {\r\n      display: none;\r\n    }\r\n    position: absolute;\r\n    bottom: 5px;\r\n    left: 5px;\r\n    width: 50px;\r\n    transition: .7s;\r\n    z-index: 2;\r\n\r\n    img {\r\n      width: 100%;\r\n    }\r\n\r\n    &.visible {\r\n      @media (max-width: 700px) {\r\n        transform: rotate(180deg);\r\n        top: 50px;\r\n        height: 50px;\r\n      }\r\n    }\r\n  }\r\n  .menu_wrapper{\r\n    position: relative;\r\n    transition: .7s;\r\n    height: 100%;\r\n    width: 30%;\r\n    margin-left: 70%;\r\n    background-color: $theme-color;\r\n    display: flex;\r\n    flex-flow: column;\r\n    transform: translateX(80%);\r\n    z-index: 2;\r\n    @media (max-width: 700px){\r\n      width: 100%;\r\n      height: 100%;\r\n      margin-left: 0;\r\n      margin-top: 30%;\r\n      transform: translateY(100vh);\r\n    }\r\n\r\n    &:hover, &.visible{\r\n      @media (max-width: 700px){\r\n        transform: translateY(0);\r\n      }\r\n      transform: translateX(0);\r\n      .up-arrow-container{\r\n        transform: rotate(180deg);\r\n      }\r\n    }\r\n    .home_title{\r\n      width: 100%;\r\n      height: fit-content;\r\n      img{\r\n        width: 100%;\r\n      }\r\n    }\r\n    .menu_container{\r\n      width: 100%;\r\n      margin-top: 20%;\r\n      @media (max-width: 700px) {\r\n        margin-top: 0;\r\n      }\r\n      .level_info_wrapper{\r\n        margin-bottom: 10%;\r\n        .level_info_label{\r\n          font-size: 200%;\r\n          color: white;\r\n        }\r\n      }\r\n      .restart_wrapper{\r\n      }\r\n    }\r\n  }\r\n}\r\n\r\n\r\n</style>"]}]}